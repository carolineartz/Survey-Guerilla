  <h3><%= @survey.title %> results:</h3>

  <% @survey_question.each do |question|%>
    <p><%= question.question_text %></p>

      <% question.choices.each do |choice|%>

          <p><%= choice.choice_text %> | <%= @survey.survey_responses.where(choice_id: choice.id).length %></p>

     <% end %>

  <% end %>














